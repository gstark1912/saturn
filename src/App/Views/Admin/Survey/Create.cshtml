@model App.Models.Admin.SurveyViewModel
@{
    ViewBag.Title = "Evaluación";
}


<form action="/Admin/Evaluation/Create" method="post" enctype="multipart/form-data">
    <div class="row">
        <div class="titles col-lg-4 col-md-4">
            <h2>@ViewBag.Title</h2>
            <h4 class="title-type">Crear</h4>
        </div>
    </div>
    <hr />
    <div class="container">
        <div class="row">
            <div class="col-md-4 col-md-offset-4 alert alert-maker">
                @Html.ValidationSummary()


                <div class="form-group">
                    @Html.LabelFor(x => x.SurveyFile)
                    @Html.TextBoxFor(x => x.SurveyFile, new { type = "file" })
                </div>
                <div class="form-group">
                    <div class="col-lg-12 col-md-12" >
                        <input type="submit" class="btn btn-primary pull-right" value="Crear" />
                    </div>
                </div>
            </div>
        </div>
    </div>
</form>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}